{
  "template": {
    "title": "Tailwind CSS",
    "description": "HTML with Tailwind CSS utility classes"
  },
  "name": "untitled",
  "version": "1.0.0",
  "editors": [
    {
      "type": "html",
      "filename": "index.html",
      "settings": { "doctype": "html5", "lang": "en" }
    },
    {
      "type": "css",
      "filename": "style.css",
      "settings": { "normalize": false, "autoprefixer": false, "tailwind": true }
    },
    {
      "type": "javascript",
      "filename": "script.js",
      "settings": { "moduleType": "module", "strictMode": true }
    }
  ],
  "globalResources": {
    "scripts": [],
    "styles": []
  }
}
