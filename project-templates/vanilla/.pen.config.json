{
  "template": {
    "title": "Vanilla",
    "description": "Plain HTML, CSS & JavaScript starter"
  },
  "name": "untitled",
  "version": "1.0.0",
  "editors": [
    {
      "type": "html",
      "filename": "index.html",
      "settings": { "doctype": "html5", "lang": "en" }
    },
    {
      "type": "css",
      "filename": "style.css",
      "settings": { "normalize": true, "autoprefixer": false }
    },
    {
      "type": "javascript",
      "filename": "script.js",
      "settings": { "moduleType": "module", "strictMode": true }
    }
  ],
  "globalResources": { "scripts": [], "styles": [] }
}
