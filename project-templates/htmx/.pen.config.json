{
  "template": {
    "title": "HTMX",
    "description": "High power tools for HTML"
  },
  "name": "htmx-project",
  "version": "1.0.0",
  "editors": [
    {
      "type": "html",
      "filename": "index.html",
      "settings": { "doctype": "html5", "lang": "en" }
    },
    {
      "type": "less",
      "filename": "style.less",
      "settings": { "normalize": true, "autoprefixer": false }
    },
    {
      "type": "javascript",
      "filename": "script.js",
      "settings": { "moduleType": "module" }
    }
  ],
  "globalResources": {
    "scripts": [
      "https://unpkg.com/htmx.org@1.9.10"
    ],
    "styles": []
  }
}
