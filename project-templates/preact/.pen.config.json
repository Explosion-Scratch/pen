{
  "template": {
    "title": "Preact (JSX)",
    "description": "Fast 3kB alternative to React"
  },
  "name": "preact-project",
  "version": "1.0.0",
  "editors": [
    {
      "type": "html",
      "filename": "index.html",
      "settings": { "doctype": "html5", "lang": "en" }
    },
    {
      "type": "less",
      "filename": "style.less",
      "settings": { "normalize": true, "autoprefixer": false }
    },
    {
      "type": "jsx",
      "filename": "script.jsx",
      "settings": { 
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    }
  ],
  "globalResources": {
    "scripts": [],
    "styles": []
  }
}
