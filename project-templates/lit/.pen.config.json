{
  "template": {
    "title": "Lit",
    "description": "Simple Web Components with Lit"
  },
  "name": "lit-project",
  "version": "1.0.0",
  "editors": [
    {
      "type": "html",
      "filename": "index.html",
      "settings": { "doctype": "html5", "lang": "en" }
    },
    {
      "type": "less",
      "filename": "style.less",
      "settings": { "normalize": true, "autoprefixer": false }
    },
    {
      "type": "javascript",
      "filename": "script.js",
      "settings": { "moduleType": "module" }
    }
  ],
  "globalResources": {
    "scripts": [],
    "styles": []
  }
}
